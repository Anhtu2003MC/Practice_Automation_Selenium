<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="AutomationPracticeSuite">
    <test name="SmokeTestGroup">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <classes>
            <class name="PracticePageTests"/>
        </classes>
    </test>

    <test name="RegressionGroup">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="PracticePageTests"/>
        </classes>
    </test>
</suite>
